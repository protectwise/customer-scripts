{
    "_version": "1.0",
    "_description": "ProtectWise Emitter Documentation Example 2",
    "fileAdapters": 
        [
            {
                "delim": "\n",
                "name": "file",
                "path": "/tmp/syslog.out",
                "rotation": {
                    "maxAgeDays": 1,
                    "maxSizeMB": 1,
                    "maxBackups": 2
                }
            }
        ],
    "subscriptions": [
        {
            "adapters": ["file"],
            "source": "event"
        }
    ]
}
